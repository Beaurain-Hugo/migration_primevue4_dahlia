{
  "name": "template-vue-3",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "npm run typecheck && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run --coverage",
    "sonar": "vitest run --coverage && npx sonar-scanner -D sonar.login=%SONAR_TEMPLATE%",
    "test:e2e": "vite build && start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "vite build && start-server-and-test preview http://127.0.0.1:5050/ 'cypress run --browser chrome'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@primeuix/themes": "^2.0.2",
    "@primevue/core": "^4.2.4",
    "@primevue/themes": "^4.2.2",
    "@types/leaflet": "^1.9.15",
    "@vee-validate/i18n": "^4.8.3",
    "@vee-validate/rules": "^4.8.4",
    "axios": "^1.3.4",
    "chart.js": "^4.4.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dom-to-pdf": "^0.3.2",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "keycloak-js": "^21.0.2",
    "leaflet": "^1.9.4",
    "mitt": "^3.0.1",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primevue": "^4.5.4",
    "vee-validate": "^4.8.4",
    "vue": "^3.4.27",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.19.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.6.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "c8": "^9.1.0",
    "cypress": "^15.8.2",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.0.0",
    "eslint-plugin-vue": "^9.25.0",
    "jsdom": "^24.0.0",
    "mocha": "^10.4.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "postcss": "^8.4.49",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "sonar-scanner": "^3.1.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.4.5",
    "vite": "^5.4.10",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^1.6.0",
    "vue-router-mock": "^0.2.0",
    "vue-tsc": "^2.0.29",
    "workbox-precaching": "^6.6.0",
    "workbox-window": "^6.6.0"
  }
}
